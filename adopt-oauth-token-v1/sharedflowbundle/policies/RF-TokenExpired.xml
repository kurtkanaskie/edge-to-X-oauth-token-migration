<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-TokenExpired">
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json">
{
    "error":"token_expired",
    "error_description":"token has expired {access_token}"
}
</Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>